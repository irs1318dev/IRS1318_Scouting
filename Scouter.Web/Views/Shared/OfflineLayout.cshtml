<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title - Scouter</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge;chrome=1" />
    @Styles.Render("~/Content/css")
    @RenderSection("styles", required: false)
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryui")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/teams")
    @Scripts.Render("~/bundles/offline")
</head>
<body class="container">
    <header>
        <div class="row">
            <div class="col-md-6">
                <a href="/"><img alt="Scouter" title="Scouter" src="/images/scouter-logo.png" class="img-responsive" /></a>
            </div>
        </div>
    </header>
    <article>
        @RenderBody()
    </article>
    <footer>
        <small>&copy; @DateTime.Now.Year Scouter by Issaquah Robotics Society</small>
    </footer>



    @RenderSection("breakout", required: false)
    @RenderSection("scripts", required: false)
</body>
</html>
